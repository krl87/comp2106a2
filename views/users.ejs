<%- include partials/header.ejs %>

<main class="container">
    <h1>Users</h1>

    <!--create table-->
    <table class="table table-striped">

        <!--loop through registered users-->
        <thead>
            <th>User ID</th><th>Username</th>
        </thead>
        <tbody>
            <% for (var i = 0; i < users.length; i++) { %>
            <tr>
                <td><%= users[i]._id %></td>
                <td><%= users[i].username %></td>
            </tr>
            <% } %>
        </tbody>
    </table>
</main>
</body>
</html>